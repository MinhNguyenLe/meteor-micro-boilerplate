{
  "name": "btaskee-backend-cs",
  "private": true,
  "scripts": {
    "dev": "npx kill-port 2200 && meteor --port=2200 --no-release-check --settings ./settings.json",
    "e2e": "CI=-1 meteor test --settings ./settings.testing.json --full-app --driver-package universe:e2e --once --raw-logs --port 4000",
    "start": "meteor run",
    "test:unit": "npx kill-port 3001 && meteor test --settings ./settings.testing.json --once --driver-package meteortesting:mocha --port 3001",
    "lint": "eslint --fix --ext .js .",
    "migrations": "meteor run ENV=-1",
    "e2e:ci": "CI=1 meteor test --settings ./settings.testing-ci.json --full-app --driver-package universe:e2e --once --raw-logs",
    "coverage:local": "BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_OUT_TEXT_SUMMARY=1 COVERAGE_OUT_HTML=1 COVERAGE_APP_FOLDER=$PWD/ nyc meteor test --settings ./settings.testing.json --once  --driver-package meteortesting:mocha --port 3002 MOCHA_TIMEOUT=30000",
    "coverage:ci": " BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_OUT_TEXT_SUMMARY=1 COVERAGE_APP_FOLDER=$PWD/ meteor test --settings ./settings.testing-ci.json --once  --driver-package meteortesting:mocha MOCHA_TIMEOUT=30000",
    "prettier-ts": "prettier --config .prettierrc.json 'imports/**/*.{tsx, ts}' 'imports/test/**/*.ts' '.prettierrc.json' 'tsconfig.json' 'package.json' '.babelrc' 'typedoc.json' 'commitlint.config.js' 'globals.d.ts' '.gitlab-ci.yml' '.spellcheckerrc.json' --write",
    "lint-ts": "eslint --fix --ext .ts,.tsx .",
    "docs": "npx typedoc --options typedoc.json",
    "cspell": "cspell --config .spellcheckerrc.json 'imports/**/*.{tsx, ts}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx --no -- commitlint --edit ${1}",
      "pre-commit": "npm run cspell && npm run prettier-ts && npm run lint-ts && npm run docs"
    }
  },
